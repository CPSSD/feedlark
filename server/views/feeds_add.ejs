<% include partials/header.ejs %>
<div class="container">
  <div class="row">
    <div class="col s12">
      <h3>Add a feed</h3>
    </div>
  </div>
  <div class="row">
    <form class="col s12" method="POST" action="/feeds/add">
      <div class="row">
        <div class="input-field col s6">
          <input id="url" name="url" type="url" class="validate" required autofocus>
          <label for="url">Feed URL</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6">
          <button class="btn waves-effect waves-light" type="submit" name="Add">Add
            <i class="material-icons right">send</i>
          </button>
        </div>
      </div>
    </form>
  </div>
</div>
<% include partials/footer.ejs %>
